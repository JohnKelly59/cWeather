</section>
<br>
<main class="form-signin">
<img src="<%=winfo[6]%>" >
  <h1 class="h3 mb-3 fw-normal">Location</h1>

  <div name="top" class="form-floating  text-white">
    <p class="form-control text-white bg-dark" id="floatingInput"> <%=winfo[0]%> &#176;F</p>
    <label for="floatingInput">Temperature</label>
  </div>
  <div class="form-floating  text-white">
    <p class="form-control text-white bg-dark" id="floatingInput"> <%=winfo[1]%> </p>
    <label for="floatingPassword">Description</label>
  </div>
  <div class="form-floating  text-white">
    <p class="form-control text-white bg-dark" id="floatingInput"> <%=winfo[2]%> &#176;F</p>
    <label for="floatingPassword">Feels Like</label>
  </div>
  <div class="form-floating  text-white">
    <p class="form-control text-white bg-dark" id="floatingInput"> <%=winfo[4]%> &#176;F</p>
    <label for="floatingPassword">Max Temperature</label>
  </div>
  <div class="form-floating  text-white">
    <p class="form-control text-white bg-dark" id="floatingInput"> <%=winfo[3]%> &#176;F</p>
    <label for="floatingPassword">Min Temperature </label>
  </div>
  <div class="form-floating  text-white">
    <p class="form-control text-white bg-dark" id="floatingInput"> <%=winfo[5]%>% </p>
    <label for="floatingPassword">Humidity</label>
  </div>
  <div name="bottom" class="form-floating  text-white">
  </div>

  </div>
  <form action="/wdata" method="post">
  <button class="w-100 btn btn-lg btn-success" type="submit">Back</button>
</form>
<br>
<footer>
<h4 class="name">jKizzy</h4>
</footer>

